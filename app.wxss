/**app.wxss**/
page{
	width: 100%;
	height: 100%;
	font-size: 30rpx;
}
.container {
  min-height: 100%;
  box-sizing: border-box;
  background: #f5f5f5;
} 
.flex-between{
	display: flex;
	justify-content: space-between;
}
.flex-start{
	display: flex;
	justify-content: flex-start;
}
.empty-data{
	padding:200rpx 0rpx;
	text-align: center;
}
.empty-data image{
	width: 300rpx;
}
.empty-data .btn{
	width: 300rpx;
	height: 39px;
	line-height: 39px;
	color: #ffffff;
	background: #ff420b;
	margin:60rpx auto;
	border-radius: 30rpx;
}